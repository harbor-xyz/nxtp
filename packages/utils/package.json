{
  "name": "@connext/nxtp-utils",
  "version": "0.2.1-rc.12",
  "description": "Common utilities for use within the @connext/nxtp-* packages",
  "author": "Connext",
  "license": "MIT",
  "scripts": {
    "dev": "yarn build",
    "lint": "eslint ./src --ext .ts --env node",
    "test": "nyc ts-mocha --check-leaks --exit --timeout 60000 'test/**/*.spec.ts'",
    "clean": "rimraf ./dist ./tsconfig.tsBuildInfo",
    "build": "tsc --build ./tsconfig.build.json",
    "verify": "yarn test && yarn clean && yarn build && yarn lint --max-warnings 0",
    "purge": "yarn clean && rimraf ./coverage ./node_modules",
    "tsp": "yarn run tsp:root \"$@\" --cwd $(pwd)",
    "version": "yarn version"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "dependencies": {
    "@gelatonetwork/relay-sdk": "2.1.0",
    "@maticnetwork/maticjs": "3.5.0",
    "@maticnetwork/maticjs-web3": "1.0.2",
    "@sinclair/typebox": "0.25.2",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "ajv": "8.11.0",
    "ajv-formats": "2.1.1",
    "axios": "1.1.3",
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "chai-subset": "1.6.0",
    "eslint": "8.26.0",
    "ethers": "5.7.2",
    "graphql-request": "5.0.0",
    "hyperid": "3.0.1",
    "merkletreejs": "0.3.0",
    "p-queue": "6.6.2",
    "pino": "8.7.0",
    "proxyquire": "2.1.3",
    "secp256k1": "4.0.3",
    "sinon-chai": "3.7.0",
    "ts-natsutil": "1.1.1"
  },
  "devDependencies": {
    "@types/chai": "4.3.3",
    "@types/chai-as-promised": "7.1.5",
    "@types/chai-subset": "1.3.3",
    "@types/mocha": "10.0.0",
    "@types/pino": "7.0.5",
    "@types/secp256k1": "4.0.3",
    "@types/sinon": "10.0.13",
    "@types/sinon-chai": "3.2.8",
    "eslint": "8.26.0",
    "mocha": "10.1.0",
    "nyc": "15.1.0",
    "rimraf": "3.0.2",
    "sinon": "14.0.1",
    "ts-mocha": "10.0.0",
    "tslib": "2.4.0",
    "typescript": "4.8.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "gitHead": "937a7cde93e6ac1e151c6374f48adf83d3fa4ec6",
  "stableVersion": "0.1.25"
}
